            params {
                input         = 'https://raw.githubusercontent.com/nf-core/test-datasets/demultiplex/samplesheet/1.3.0/fqtk-samplesheet.csv'
                demultiplexer = 'fqtk'
                outdir        = "$outputDir"
            }
